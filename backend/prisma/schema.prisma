// backend/prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Client {
  id     String  @id @default(uuid())
  name   String
  email  String  @unique
  status Boolean @default(true) // true para ativo, false para inativo
}